{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-useless-constructor": "error",
    "camelcase": "off",
    "no-unused-vars": "warn",
    "callback-return": ["warn", ["callback", "next"]]
  }
}
